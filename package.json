{
  "name": "lexora-website",
  "private": true,
  "type": "module",
  "version": "v0.0.1",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "preview": "pnpm run build && npx wrangler dev",
    "postinstall": "nuxt prepare && pnpm rebuild better-sqlite3",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "nuxt typecheck",
    "test": "echo 'No tests specified' && exit 0",
    "lighthouse": "lhci autorun",
    "analyze": "nuxt analyze",
    "deploy": "npx wrangler deploy --env production",
    "deploy:staging": "npx wrangler deploy --env staging",
    "tail": "npx wrangler tail --env production",
    "tail:staging": "npx wrangler tail --env staging",
    "cf-typegen": "npx wrangler types"
  },
  "dependencies": {
    "@iconify-json/circle-flags": "^1.2.8",
    "@iconify-json/lucide": "^1.2.66",
    "@iconify-json/simple-icons": "^1.2.50",
    "@nuxt/content": "^3.7.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "2.0.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "^4.0.0",
    "@nuxtjs/i18n": "^10.0.3",
    "@nuxtjs/seo": "^3.1.0",
    "@saslavik/nuxt-gtm": "^0.1.2",
    "@unhead/vue": "^2.0.14",
    "@vueuse/nuxt": "^13.6.0",
    "nuxt": "^4.1.1",
    "vue": "^3.5.21",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@nuxt/eslint": "^1.9.0",
    "better-sqlite3": "^12.2.0",
    "cssnano": "^7.1.0",
    "eslint": "^9.35.0",
    "nitro-cloudflare-dev": "^0.2.2",
    "sharp": "0.34.3",
    "typescript": "^5.9.2",
    "vue-tsc": "^3.0.6",
    "wrangler": "^4.33.1"
  },
  "pnpm": {
    "overrides": {
      "sharp": "0.34.3"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  }
}
